## The function is currently defined as
function (files) 
{
    xlstabs <- vector("list", length = length(files))
    names(xlstabs) <- files
    for (i in 1:length(files)) xlstabs[[i]] <- read.xlsx(files[i], 
        sheetIndex = 1, header = F)
    return(raw_tabs = xlstabs)
  }
}
